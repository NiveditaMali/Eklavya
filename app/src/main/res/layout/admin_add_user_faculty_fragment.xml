<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/adminadduserfaculty"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.adminadduserfaculty.AdminAddUserFacultyFragment">


    <Button
        android:id="@+id/btnaaufchoosefile"
        android:layout_width="141dp"
        android:layout_height="51dp"
        android:layout_marginTop="52dp"
        android:text="Choose File"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtaauffilepath"
        android:layout_width="380dp"
        android:layout_height="38dp"
        android:layout_marginTop="51dp"
        android:text="File Path"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnaaufchoosefile" />

    <Button
        android:id="@+id/btnaaufupload"
        android:layout_width="141dp"
        android:layout_height="51dp"
        android:layout_marginTop="51dp"
        android:text="Upload"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtaauffilepath" />

</androidx.constraintlayout.widget.ConstraintLayout>
