<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/adminadduserstudent"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.adminadduserstudent.AdminAddUserStudentFragment">

    <Button
        android:id="@+id/btnaauschoosefile"
        android:layout_width="141dp"
        android:layout_height="51dp"
        android:layout_marginTop="53dp"
        android:text="Choose File"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinaausclass" />

    <Spinner
        android:id="@+id/spinaausclass"
        android:layout_width="279dp"
        android:layout_height="42dp"
        android:layout_marginTop="16dp"
        android:text="File Path"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtaausclass" />

    <TextView
        android:id="@+id/txtaausfilepath"
        android:layout_width="380dp"
        android:layout_height="38dp"
        android:layout_marginTop="51dp"
        android:text="File Path"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnaauschoosefile" />

    <Button
        android:id="@+id/btnaausupload"
        android:layout_width="141dp"
        android:layout_height="51dp"
        android:layout_marginTop="51dp"
        android:text="Upload"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtaausfilepath" />

    <TextView
        android:id="@+id/txtaausclass"
        android:layout_width="176dp"
        android:layout_height="27dp"
        android:layout_marginTop="52dp"
        android:text="Class"
        android:textAlignment="textStart"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.28"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
