<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/facultyschedule"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.facultyschedule.FacultyScheduleFragment">

    <TextView
        android:id="@+id/day"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="FacultyScheduleFragment"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/facday"
        android:layout_width="59dp"
        android:layout_height="33dp"
        android:layout_marginTop="8dp"
        android:text="Day"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.014" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fabfacschedule"
        android:layout_width="59dp"
        android:layout_height="56dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:clickable="true"
        android:src="@drawable/ic_launcher_calender_foreground"
        app:backgroundTint="#DEB5A6"
        app:elevation="6dp"
        app:fabSize="normal"
        app:hoveredFocusedTranslationZ="6dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.929"
        app:layout_constraintStart_toEndOf="@+id/day"
        app:layout_constraintTop_toTopOf="parent"
        app:maxImageSize="80dp" />

</androidx.constraintlayout.widget.ConstraintLayout>
